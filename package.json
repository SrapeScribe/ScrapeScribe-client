{
	"name": "scrapescribe-frontend",
	"version": "0.0.1",
	"license": "Proprietary",
	"type": "module",
	"scripts": {
		"wasm:build": "cd src/wasm/scraping-instructions && wasm-pack build",
		"dev": "npm run wasm:build && vite dev",
		"build": "npm run wasm:build && vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check .",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage"
	},
	"devDependencies": {
		"@lucide/svelte": "^0.482.0",
		"@sveltejs/adapter-auto": "^3.3.1",
		"@sveltejs/adapter-static": "^3.0.6",
		"@sveltejs/kit": "^2.19.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@tailwindcss/aspect-ratio": "^0.4.2",
		"@tailwindcss/container-queries": "^0.1.1",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"autoprefixer": "^10.4.20",
		"bits-ui": "^1.3.12",
		"clsx": "^2.1.1",
		"formsnap": "^2.0.0",
		"lucide-svelte": "^0.460.1",
		"mode-watcher": "^0.5.0",
		"prettier": "^3.3.2",
		"prettier-plugin-svelte": "^3.2.6",
		"prettier-plugin-tailwindcss": "^0.6.5",
		"svelte": "^5.20.4",
		"svelte-check": "^4.0.9",
		"svelte-sonner": "^0.3.28",
		"sveltekit-superforms": "^2.24.0",
		"tailwind-merge": "^2.5.4",
		"tailwind-variants": "^0.2.1",
		"tailwindcss": "^3.4.9",
		"typescript": "^5.6.3",
		"vite": "^5.4.14",
		"vite-plugin-top-level-await": "^1.5.0",
		"vite-plugin-wasm": "^3.4.1",
		"vitest": "^2.0.4",
		"zod": "^3.24.2"
	},
	"dependencies": {
		"@aws-amplify/auth": "^6.11.3",
		"aws-amplify": "^6.13.3",
		"tailwindcss-animate": "^1.0.7"
	}
}
